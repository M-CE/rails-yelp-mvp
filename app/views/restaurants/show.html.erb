<h1>Restaurant List</h1>
<hr>
<%=link_to "List Restaurants", restaurants_path, class: "btn btn-primary" %>
<hr>
<p>name: <%= @restaurant.name %></p>
<p>address: <%= @restaurant.address %></p>
<p>phone: <%= @restaurant.phone_number %></p>
<p>category: <%= @restaurant.category %></p>
<hr>
<%= link_to "Add Review", new_restaurant_review_path(@restaurant), class: "btn btn-primary" %>
<%= link_to "Check Reviews", restaurant_reviews_path(@restaurant), class: "btn btn-primary" %>
